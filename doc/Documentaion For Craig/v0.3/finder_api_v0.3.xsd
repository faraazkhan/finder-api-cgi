  <xs:element name="PublicOptionsAPIResponse">
    <xs:complexType>
      <xs:choice>
        <xs:sequence>
          <xs:element name="PublicOptions">
            <xs:annotation>
              <xs:documentation>Public Options</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="PublicOption" minOccurs="1" maxOccurs="unbounded" >
                  <xs:annotation>
                    <xs:documentation>Public Option</xs:documentation>
                  </xs:annotation>
                    <xs:complexType>
                        <xs:choice>
                            <xs:element name="ChipData" type="ChipOrMcdDataType" >
                        <xs:annotation>
                          <xs:documentation>Chip Data</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                            <xs:element name="MedicaidData" type="ChipOrMcdDataType" >
                        <xs:annotation>
                          <xs:documentation>Medicaid Data</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                            <xs:element name="HighriskData" type="HighriskDataType" >
                        <xs:annotation>
                          <xs:documentation>Highrisk Data</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                            <xs:element name="TerritoryData" type="TerritoryDataType" >
                        <xs:annotation>
                          <xs:documentation>Territory Data</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                            <xs:element name="IndividualAndFamilyPlanData" type="IndividualAndFamilyPlanDataType">
                        <xs:annotation>
                          <xs:documentation>Individual and Family Plan Data</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                            <xs:element name="SmallBusinessProductData" type="SmallBusinessProductDataType">
                        <xs:annotation>
                          <xs:documentation>Small Business Product Data</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                            <xs:element name="PublicOptionData" type="GenericPublicOptionDataType" >
                        <xs:annotation>
                          <xs:documentation>Public Option Data</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                        </xs:choice>
                    </xs:complexType>
                </xs:element>
                <xs:element name="CHIPAndMedicaidBenefitGroups" type="CHIPAndMedicaidBenefitGroupsType" minOccurs="0" >
                  <xs:annotation>
                    <xs:documentation>CHIP and Medicaid Benefit Groups</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="CHIPAndMedicaidDisclaimers" type="CHIPAndMedicaidDisclaimersType" minOccurs="0">
                  <xs:annotation>
                    <xs:documentation>CHIP and Medicaid Disclaimers</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:sequence>
          <xs:element name="InvalidRequestReason" type="xs:string">
            <xs:annotation>
              <xs:documentation>Reason for invalid request.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="InvalidRequestDetails" type="xs:string" minOccurs="1" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Details of invalid request.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:choice>
    </xs:complexType>
 </xs:element>
 
  <xs:element name="PrivateOptionsAPIResponse">
    <xs:complexType>
        <xs:choice>
            <xs:element name="CountiesForPostalCodeResponse" type="ZipCodeValidationResponse" >
          <xs:annotation>
            <xs:documentation>Counties For Postal Code</xs:documentation>
          </xs:annotation>
        </xs:element>
            <xs:element name="PlansForIndividualOrFamilyResponse" type="IndividualPlanQuoteResponse" >
          <xs:annotation>
            <xs:documentation>Plans for Individual or Family</xs:documentation>
          </xs:annotation>
        </xs:element>
            <xs:element name="PlanDetailsForIndividualOrFamilyResponse" type="IndividualPlanBenefitResponse">
          <xs:annotation>
            <xs:documentation>Plan Details for Individual or Family</xs:documentation>
          </xs:annotation>
        </xs:element>
            <xs:element name="ProductsForSmallGroupResponse" type="SmallGroupProductQuoteResponse">
          <xs:annotation>
            <xs:documentation>Products for Small Group</xs:documentation>
          </xs:annotation>
        </xs:element>
            <xs:element name="ProductDetailsForSmallGroupResponse" type="SmallGroupProductBenefitResponse">
          <xs:annotation>
            <xs:documentation>Product Details for Small Group</xs:documentation>
          </xs:annotation>
        </xs:element>
        </xs:choice>
    </xs:complexType>
  </xs:element>


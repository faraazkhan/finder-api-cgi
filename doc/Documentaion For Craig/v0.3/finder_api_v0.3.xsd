  <!-- ******************************************** -->
  <!-- REQUEST -->
  <!-- ******************************************** -->
  <!-- ******************************************** -->
  <!-- ******************************************** -->
  <!-- ******************************************** -->
  <!-- ******************************************** -->
  <!-- ******************************************** -->
  <!-- ******************************************** -->
  <!-- ******************************************** -->

  <xs:element name="PublicOptionsAPIRequest">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="RequesterInfoData" type="RequesterInfoDataType" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Requester Info</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="StateOrTerritory" type="StateOrTerritoryType">
          <xs:annotation>
            <xs:documentation>State or Territory</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:choice minOccurs="0" maxOccurs="1">
          <xs:sequence>
            <xs:choice>
              <xs:element name="FamilyAndChildrenAudienceRequest">
                <xs:annotation>
                  <xs:documentation>Family and Children Audience Request</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Situation">
                      <xs:annotation>
                        <xs:documentation>Situation</xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="familylosingworkinsurance">
                            <xs:annotation>
                              <xs:documentation>My family and I are losing the health insurance we have through work</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="familyrejectedinsurance">
                            <xs:annotation>
                              <xs:documentation>My family and I tried to get health insurance, but we were rejected for coverage</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="familyneedinsurance">
                            <xs:annotation>
                              <xs:documentation>My family and I need health insurance for another reason</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="AgeRange" type="AgeRangeFullType">
                      <xs:annotation>
                        <xs:documentation>Age Range</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="HealthyIndividualAudienceRequest">
                <xs:annotation>
                  <xs:documentation>Healthy Individual Audience Request</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Situation">
                      <xs:annotation>
                        <xs:documentation>Situation</xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="individuallosingworkinsurance">
                            <xs:annotation>
                              <xs:documentation>I'm losing the health insurance I have through work.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="individualneedinsurance">
                            <xs:annotation>
                              <xs:documentation>I need health insurance.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="AgeRange" type="AgeRangeFullType">
                      <xs:annotation>
                        <xs:documentation>Age Range</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="IndividualWithMedicalConditionAudienceRequest">
                <xs:annotation>
                  <xs:documentation>Individual With Medical Condition Audience Request</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Situation">
                      <xs:annotation>
                        <xs:documentation>Situation</xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="medicallosingworkinsurance">
                            <xs:annotation>
                              <xs:documentation>I'm losing the health insurance I have through work.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="medicalrejectedinsurance">
                            <xs:annotation>
                              <xs:documentation>I've tried to get health insurance, but I was rejected for coverage because of my medical condition.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="medicalneedinsurance">
                            <xs:annotation>
                              <xs:documentation>I need health insurance.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="AgeRange" type="AgeRangeFullType">
                      <xs:annotation>
                        <xs:documentation>Age Range</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="PregnantWomanAudienceRequest">
                <xs:annotation>
                  <xs:documentation>Pregnant Woman Audience Request</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Situation">
                      <xs:annotation>
                        <xs:documentation>Situation</xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="pregnantlosingworkinsurance">
                            <xs:annotation>
                              <xs:documentation>I'm losing the health insurance I have through work.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="pregnantrejectedinsurance">
                            <xs:annotation>
                              <xs:documentation>I've tried to get health insurance, but I was rejected for coverage because of my medical condition.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="pregnantneedinsurance">
                            <xs:annotation>
                              <xs:documentation>I need health insurance.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="AgeRange">
                      <xs:annotation>
                        <xs:documentation>Age Range</xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="18 or Under">
                            <xs:annotation>
                              <xs:documentation>18 or Under</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="19-25">
                            <xs:annotation>
                              <xs:documentation>19-25</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="26-64">
                            <xs:annotation>
                              <xs:documentation>26-64</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>

                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="PersonWithDisabilityAudienceRequest">
                <xs:annotation>
                  <xs:documentation>Person With Disability Audience Request</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Situation">
                      <xs:annotation>
                        <xs:documentation>Situation</xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="disabilitylosingworkinsurance">
                            <xs:annotation>
                              <xs:documentation>I'm losing the health insurance I have through work.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="disabilityrejectedinsurance">
                            <xs:annotation>
                              <xs:documentation>I've tried to get health insurance, but I was rejected for coverage because of my medical condition.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="disabilityneedinsurance">
                            <xs:annotation>
                              <xs:documentation>I need health insurance.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="disabilitylongtermcare">
                            <xs:annotation>
                              <xs:documentation>I need help paying for long-term care.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="AgeRange" type="AgeRangeFullType">
                      <xs:annotation>
                        <xs:documentation>Age Range</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="SeniorAudienceRequest">
                <xs:annotation>
                  <xs:documentation>Senior Audience Request</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Situation">
                      <xs:annotation>
                        <xs:documentation>Situation</xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="seniorlearnaboutlaw">
                            <xs:annotation>
                              <xs:documentation>I have Medicare and want to learn how the new health law will help me.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="seniorinsurancenomedicare">
                            <xs:annotation>
                              <xs:documentation>I don't have Medicare and want to learn about other health insurance options.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="seniorinsurancebenefits">
                            <xs:annotation>
                              <xs:documentation>I want to know more about insurance for benefits that are not covered by Medicare.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="seniorhowmedicare">
                            <xs:annotation>
                              <xs:documentation>How do I get Medicare?</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="seniorcantafford">
                            <xs:annotation>
                              <xs:documentation>I can't afford to pay for my costs under Medicare.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="seniorlongtermcare">
                            <xs:annotation>
                              <xs:documentation>I need help paying for long-term care.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="YoungAdultAudienceRequest">
                <xs:annotation>
                  <xs:documentation>Young Adult Audience Request</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Situation">
                      <xs:annotation>
                        <xs:documentation>Situation</xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="studentlosingschoolinsurance">
                            <xs:annotation>
                              <xs:documentation>I'm losing the student health insurance I had through my school</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="studentlosingparentalinsurance">
                            <xs:annotation>
                              <xs:documentation>I'm losing the health insurance I had through work (or my parent's work).</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="studentrejectedcondition">
                            <xs:annotation>
                              <xs:documentation>I've tried to get health insurance, but I was rejected for coverage because I'm sick or disabled.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="studentneedinsurance">
                            <xs:annotation>
                              <xs:documentation>I need health insurance.</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="AgeRange">
                      <xs:annotation>
                        <xs:documentation>Age Range</xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="18 or Under">
                            <xs:annotation>
                              <xs:documentation>18 or Under</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                          <xs:enumeration value="19-25">
                            <xs:annotation>
                              <xs:documentation>19-25</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:choice>
            <xs:element name="MedicalConditionOrHealthProblemIndicator" type="xs:boolean" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>Medical condition or health problem</xs:documentation>
              </xs:annotation>
            </xs:element>
            <!-- A value of of true will be implied for a an IndividualWithMedicalCondition -->
            <xs:element name="DisabilityIndicator" type="xs:boolean" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>Disability</xs:documentation>
              </xs:annotation>
            </xs:element>
            <!-- A value of of true will be implied for a an PersonWithDisability -->
            <xs:element name="BreastOrCervicalCancerIndicator" type="xs:boolean" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>Breast or Cervical Cancer</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DependentUnder21Indicator" type="xs:boolean" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>dependent under 21</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="NursingHomeOrLongTermCareIndicator" type="xs:boolean" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>Nursing home or long term care</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="SpecialHealthcareNeedIndicator" type="xs:boolean" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>Special healthcare need</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="PregnancyIndicator" type="xs:boolean" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>Pregnancy</xs:documentation>
              </xs:annotation>
            </xs:element>
            <!-- A value of of true will be implied for a PregnantWoman and ignored for Senior -->
            <xs:element name="VeteranStatusIndicator" type="xs:boolean" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>Veteran status</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="AmericanIndianOrAlaskanNativeIndicator" type="xs:boolean" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>American Indian or Alaskan Native</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="IsItDifficultForYouOrYourFamilyMemberToAffordInsuranceIndicator" type="xs:boolean" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>Is it difficult to afford insurance</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
          <xs:element name="SmallEmployerOrSelfEmployedAudienceRequest">
            <xs:annotation>
              <xs:documentation>Small Employed Or Self Employed Audience Request</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="Situation">
                  <xs:annotation>
                    <xs:documentation>Situation</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                    <xs:restriction base="xs:string">
                      <xs:enumeration value="employerinsurance">
                        <xs:annotation>
                          <xs:documentation>I need health unsurance for my employees.</xs:documentation>
                        </xs:annotation>
                      </xs:enumeration>
                      <xs:enumeration value="selfemployedinsurance">
                        <xs:annotation>
                          <xs:documentation>I'm self-employed with no other employees.</xs:documentation>
                        </xs:annotation>
                      </xs:enumeration>
                    </xs:restriction>
                  </xs:simpleType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="PrivateOptionsAPIRequest">
    <xs:complexType>
        <xs:sequence>
            <xs:element name="RequesterInfoData" type="RequesterInfoDataType" minOccurs="0">  
          <xs:annotation>
            <xs:documentation>Requester Info</xs:documentation>
          </xs:annotation>
        </xs:element>
            <xs:choice>
                <xs:element name="CountiesForPostalCodeRequest" type="ZipCodeValidationRequest">
            <xs:annotation>
              <xs:documentation>Counties for Postal Code</xs:documentation>
            </xs:annotation>
          </xs:element>
                <xs:element name="PlansForIndividualOrFamilyRequest" type="IndividualPlanQuoteRequest" >
            <xs:annotation>
              <xs:documentation>Plans for Individual or Family</xs:documentation>
            </xs:annotation>
          </xs:element>
                <xs:element name="PlanDetailsForIndividualOrFamilyRequest" type="IndividualPlanBenefitRequest">
            <xs:annotation>
              <xs:documentation>Plan Details for Individual or Family</xs:documentation>
            </xs:annotation>
          </xs:element>
                <xs:element name="ProductsForSmallGroupRequest" type="SmallGroupProductQuoteRequest">
            <xs:annotation>
              <xs:documentation>Products for Small Group</xs:documentation>
            </xs:annotation>
          </xs:element>
                <xs:element name="ProductDetailsForSmallGroupRequest" type="SmallGroupProductBenefitRequest">
            <xs:annotation>
              <xs:documentation>Product Details for Small Group</xs:documentation>
            </xs:annotation>
          </xs:element>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  


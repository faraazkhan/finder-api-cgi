  <xs:complexType name="RequesterInfoDataType">
    <xs:sequence>
        <xs:element name="OrganizationName" type="xs:string">
        <xs:annotation>
          <xs:documentation>Name of organization requesting info.</xs:documentation>
        </xs:annotation>
      </xs:element>
        <xs:element name="ContactEmail" type="xs:string">
        <xs:annotation>
          <xs:documentation>Contact email for requestor.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="ChipOrMcdDataType">
    <xs:sequence>
      <xs:element name="ContactInformation" type="StateMedicaidOrChipInfoType" minOccurs="0">
      <xs:annotation>
        <xs:documentation>Contact Information for CHIP or State Medicaid program</xs:documentation>
      </xs:annotation>
    </xs:element>
      <xs:element name="Programs" type="CHIPAndMedicaidProgramsType" minOccurs="0">
      <xs:annotation>
        <xs:documentation>Programs for CHIP or State Medicaid Program</xs:documentation>
      </xs:annotation>
    </xs:element>
  </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="IndividualAndFamilyPlanDataType">
    <xs:sequence>
      <xs:element name="IndividualMarketURL" type="URLType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>URL for the individual market</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="SmallBusinessProductDataType">
    <xs:sequence>
      <xs:element name="GroupMarketURL" type="URLType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>URL for Group Market</xs:documentation>
        </xs:annotation>
      </xs:element>
